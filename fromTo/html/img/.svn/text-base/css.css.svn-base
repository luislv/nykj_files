@charset "UTF-8";
/* CSS Document */
body{ font:12px/1.5 Tahoma, Helvetica, Arial, '閿熸枻鎷烽敓鏂ゆ嫹','Microsoft YaHei', sans-serif; color:#333;min-height:400px}
body,hr,p,blockquote,dl,dt,dd,div,ul,ol,li,pre,form,fieldset,legend,button,input,textarea,tr,th,td{ margin:0; padding:0;}

select{ height:30px; border:1px solid #BFD8DF; padding:5px;}
image{ border:0px;}
li{ display:block;}
h1,h2,h3,h4,h5,h6{ margin:10px 0px; font-family:"Microsoft Yahei"; }
table{ border:0px;}
a{ text-decoration:none; color:#0056AB;cursor:pointer;display:inline-block;text-align:center;line-height:24px;}
a:hover{ text-decoration:none; font-weight:bold;}
.green{ color:green; }
a.red:hover, a.red, .red{ color:red;}
.clear{ clear:both;}
.fl{ float:left;}
.fr{ float:right;}
.select{ width:162px;}


/**导航栏***/
.leftnav{height:100%;
background: #34C5F0 linear-gradient(to right, #34C5F0 0px, #fcecff 100%) repeat-y scroll 0 0;
}
.nav_logo{height:90px; width:100%;background: url("../html/img/logo.png") no-repeat center top;}
.nav_menu a{color:#FFF; font-weight: 500; font-size:14px;letter-spacing:8px; width:100%; padding-top:32px; border-bottom: #D0F2FF 1px dotted; }
.nav_menu a:hover,.nav_footer a:hover,.nav_footer a.cur{font-weight:bold; background: #A2D4FF  linear-gradient(to right, #A2D4FF 0px, #f7f7f7 100%) repeat-y scroll 0 0;}

.nav_footer{ width:65px; height:30px; position: absolute; bottom:5px; padding-bottom:20px;}
.nav_footer a{color:#FFF; width:100%;}

.topnav{ width:100%; height:102px; min-width:1030px; overflow:hidden; background: #0190d4 none no-repeat left center;}
.topnav .nav_menu a{ height:100%; width:70px;  padding-top:3px; border-left: #D0F2FF 1px dotted; border-bottom: #D0F2FF 0px dotted; font-weight: 100; font-size:14px;letter-spacing:1px;}
.topnav .nav_menu a:hover{background: #34a6dd none repeat scroll 0 0;}
.topnav .nav_footer{right:5px;}
.topnav img{ border: 0 none; display:block; padding:10px 22px 2px;}

#leftmenus {height: 100%; overflow: hidden; border:#34C5F0 1px groove; z-index:99px; background-color:#FFF; width:173px;}
#leftmenus h3 {width:100%;background: #eef1f6 url("../html/img/admin/icon_down.gif") repeat scroll 0 0;border-bottom: 1px solid #c1c4c9;cursor: pointer;font: bold 14px/30px 宋体;height: 30px;margin: 0;padding-left: 20px;
}
#leftmenus h3.cur {background: #0190d4 url("../html/img/admin/icon_up.gif") repeat scroll 0 0;color: #fff;}

.menu-treeview{display: block; overflow: hidden; padding: 0;}
.menu-treeview a{width:100%; height: 25px; line-height:24px; background:#F3F3F3;margin-bottom: 1px;padding:0px 10px 0px 40px; text-align:left;}
.menu-treeview a:hover {font-weight: normal; background: #e5e5e5 url("../html/img/admin/page.png") no-repeat scroll 16px 5px;}
.menu-treeview a.cur {background: #e0ecff url("../html/img/admin/page.png") no-repeat scroll 16px 5px;}


.page-header{padding-top:0px; height:35px; line-height:24px;overflow:hidden;background: rgba(0, 0, 0, 0) linear-gradient(to bottom, #e0ecff 0px, #FCFCFC 100%) repeat-x scroll 0 0}
.page-title-btn{position: absolute;right: 5px;top: auto; bottom:2px; }
.page-title-btn a{background: transparent url('button_bg.png') repeat-x scroll 0 0;  border: 1px #aac8cc solid; height:24px;padding:0px;}

/***easyui***/
.panel-header, .panel-body {border-width: 0px;/*border-bottom-width: 1px;*/}
.tabs {
    background: #f4f4f4 linear-gradient(to bottom, #FCFCFC 0px, #e0ecff 100%) repeat-x scroll 0 0;
    border-bottom: 1px solid #bbb;
    font-size: 12px;
    height: 26px;
    list-style-type: none;
    margin: 0;
    padding: 3px 0 0 15px;
   /* width: 100%;*/
}
.tabs li {
    -moz-user-select: none;
    background-clip: padding-box;
    background-color: #fff;
    background-image: linear-gradient(to top, #eee 0%, #fff 50%);
    border: 1px solid #bbb;
    border-top-left-radius: 5px;
    border-top-right-radius: 5px;
    display: inline-block;
    float: left;
    margin-top: -2px;
    margin-right: 4px;
    padding: 0;
    position: relative;
}
.tabs li a.tabs-inner {
    color: #15428b;
	border-width: 0;
    display: inline-block;
    height: 25px;
    line-height: 25px;
    margin: 0;
    padding: 0 10px;
    text-align: center;
    text-decoration: none;
    white-space: nowrap;
    background: rgba(0, 0, 0, 0) none repeat-x scroll 0 0;
}
.tabs li a.tabs-inner:hover {
    -moz-user-select: none;
    background-clip: padding-box;
    background-color: #eee;
    background-image: linear-gradient(to top, #ccc 0%, #eee 50%);
}
.tabs li.tabs-selected {
    border-color: #bbb #bbb #fff;
    border-image: none;
    border-style: solid;
    border-width: 1px;
}
.tabs li.tabs-selected a.tabs-inner {
    background-image: linear-gradient(to bottom,#e0ecff 0px,  #fff 100%);
    /*background: #fff none repeat scroll 0 0;*/
    border-bottom: 1px solid #fff;
    border-top-left-radius: 5px;
    border-top-right-radius: 5px;
    color: #000;
    outline: medium none;
}
.tabs li.tabs-selected a.tabs-inner:hover {
    cursor: default;
}
.global_tit {
    /*background-color: #e0ecff;
    border-bottom: 1px solid #bad4f7;*/
    color: #00008b;
    font-family: 微软雅黑;
    font-size: 18px;
    letter-spacing: 5px;
    line-height: 18px;
    margin-bottom: 5px;
    padding: 6px 0;
    text-align: center;
    width: 300px;
    position: relative;
	left: 40%;
	top: -32px;
    z-index: 999;
}

/**页面**/
.bottom_float {
    background-color: #fff;
    border-top: 1px solid #eaeaea;
    bottom: 0;
    padding: 5px 0;
    position: fixed;
    text-align: center;
    width: 100%;
    z-index: 999;
}
.panel-tool-back {
    background: rgba(0, 0, 0, 0) url("panel_tools.png") no-repeat scroll 0 -32px;
}
.table {
    background: #fff none repeat scroll 0 0;
    border-left: 1px solid #eaeaea;
    border-spacing: 0;
    border-top: 1px solid #eaeaea;
    margin: 0px;
    padding: 0;
    width: 100%;
}
.table_min {
    min-width: 1000px;
}
.table_header {
    background: #f4f4f4 linear-gradient(to bottom, #FCFCFC 0px, #e0ecff 100%) repeat-x scroll 0 0;
    border-bottom: 1px dashed #ccc;
    font-size: 14px;
    font-weight: bold;
    height: 30px;
    line-height: 24px;
    padding-left: 10px;
	text-align: left;
	margin-top:3px;
}
.table_header_bottom {
    background: #f4f4f4 linear-gradient(to bottom, #e0ecff 0px, #FCFCFC 100%) repeat-x scroll 0 0;
    border-bottom: 1px dashed #ccc;
    font-size: 14px;
    font-weight: bold;
    height: 30px;
    line-height: 24px;
    padding-left: 10px;
	text-align: left;
}
.right-tool-buts{ float:right; padding-right:5px; font-weight:500;}
.table td, .table th {
    background-color: #eef1f6;
    border-bottom: 1px solid #fff;
    border-right: 1px solid #fff;
    font-size: 12px;
	 line-height:24px;
    padding: 2px 5px;
}
.table .t_tip {
    font-size: 14px;
    padding-bottom: 20px;
}
.table th, .table .t_title, .table .t_title_l {
    background: #e0ecff none repeat scroll 0 0;
    color: #333;
    font-weight: bold;
    line-height: 28px;
    text-align: center;
    vertical-align: middle;
}
.table .t_title_l {
    text-align: left;
}
.table th {
    color: #15428b;
    text-align: left;
}
.table_blank {
    border: 0 none;
    width: 100%;
}
.table_blank td {
    border: 0 none;
}
.table_blank .t_title {
    background: #fff none repeat scroll 0 0;
    color: #333;
    font-weight: bold;
    line-height: 28px;
    text-align: right;
}

